Ans 1
import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("enter a,b and c");
        double a = sc.nextDouble();
         double b = sc.nextDouble();
        double c = sc.nextDouble();
        if (a==0){
            System.out.println("a cannot be zero");
        }
         double disc=b*b-4*a*c;
       
        if(disc>0){
              double root1 = (-b + Math.sqrt(disc)) / (2 * a);
               double root2 = (-b - Math.sqrt(disc)) / (2 * a);
               System.out.println("the root1 is"+root1);
               System.out.println("the root2 is"+root2);
        }
        else if (disc == 0) {
             double root = -b / (2 * a);
               System.out.println("the root is"+root);
        }
        else{
            System.out.println("no real soln");
        }
    }
}
Ans 2
import java.util.Scanner;
public class Main{
    public static int fibrec(int n){
        if(n <= 1){
            return n;
        }
        return fibrec(n-1) + fibrec(n-2);
    }
    public static int fibite(int n){
        if(n<=1){
            return n;
        }
        int prev =0,curr=1;
        for(int i=2;i<=n;i++){
            int next=prev+curr;
            prev=curr;
            curr=next;
        }
        return curr;
    }
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("enter possition ");
        int n=sc.nextInt();
        int rec=fibrec(n);
        System.out.println("the recursive ans is:"+rec);
        int iter=fibite(n);
         System.out.println("the iterative ans is:"+iter);
       
    }
}
Ans 3
import java.util.Scanner;
public class ass3{
 
    public static void main(String[] args){
   
        Scanner sc=new Scanner(System.in);
        System.out.println("enter till where you want prime numbers ");
        int n=sc.nextInt();
       
        for(int num=2;num<=n;num++){
            boolean isprime=true;
            for(int i=2;i*i<=num;i++ ){
            if(num % i==0){
                isprime=false;
                break;
            }
            }
            if(isprime){
                System.out.println("the num are :"+num);
            }
        }
       
    }
}
Ans 4
import java.util.Scanner;
public class Main{
 
    public static void main(String[] args){
   
        Scanner sc=new Scanner(System.in);
        System.out.println("enter number ");
        int n=sc.nextInt();
         System.out.println("the multiplication table of "+n);
        for(int i=1;i<=10;i++){
            System.out.println(n*i+" ");
        }
       
    }
}
Ans 5
import java.util.Scanner;
public class Main{
 
    public static void main(String[] args){
   
        Scanner sc=new Scanner(System.in);
        System.out.println("enter number ");
        int n=sc.nextInt();
       
        for(int i=1;i<=n;i++){
            for(int j=1;j<=i;j++){
                System.out.print("*");
            }
              System.out.println();
        }
       
    }
}
