class stud {
    String name;
    int age;
    String maakanaam;
stud(String n,int a,String m){
    
        this.name = n;
        this.age = a;
        this.maakanaam = m;

}
stud(String na,int ag){
    this(na,ag,"unknown");
    System.out.println("another type");
}
    void display() {
        System.out.println("name: " + this.name);
        System.out.println("age: " + age);
        System.out.println("maakanaam: " + maakanaam);
    }
}

public class Main {
    public static void main(String[] args) {

        stud s1 = new stud("geetika",19,"pinky");
        s1.display();
        stud s2=new stud("geet",19);
        s2.display();
    }
}
